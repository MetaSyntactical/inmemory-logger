{
  "name": "metasyntactical/inmemory-logger",
  "description": "PSR-3 compatible InMemory Logger with ability to fetch logged entries from memory.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Kreuer",
      "email": "d.kreuer@danielkreuer.com"
    }
  ],
  "require": {
    "php": "8.0.*||8.1.*",
    "psr/log": "^1.0|^2.0|3.0",
    "regex-guard/regex-guard": "^1.1",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "phpspec/phpspec": "^7.2"
  },
  "provide": {
    "psr/log-implementation": "1.0|2.0|3.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "MetaSyntactical\\Log\\InMemoryLogger\\": "src/"
    }
  }
}
